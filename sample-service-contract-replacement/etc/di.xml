    <virtualType name="Magento\SampleServiceContractReplacement\Model\ActiveCartRepository" type="Magento\SampleServiceContractReplacement\Model\CartRepository">
        <arguments>
            <argument name="quoteRepository" xsi:type="object">Magento\SampleServiceContractReplacement\Model\QuoteRepository</argument>
        </arguments>
    </virtualType>
    <preference for="Magento\GiftMessage\Api\CartRepositoryInterface" type="Magento\SampleServiceContractReplacement\Model\ActiveCartRepository"/>
    <virtualType name="Magento\SampleServiceContractReplacement\Model\ActiveItemRepository" type="Magento\SampleServiceContractReplacement\Model\ItemRepository">
